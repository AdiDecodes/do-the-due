{
	"name": "dothedue",
	"version": "1.0.3",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/types/index.d.ts",
	"scripts": {
		"build:css": "npx tailwindcss -i ./src/styles/tailwind.css -o ./dist/styles/tailwind.css --minify",
		"build": "npm run copy-assets && npm run build:css && npm run build:all && npm run obfuscate",
		"build:all": "npm run build:cjs && npm run build:esm",
		"build:cjs": "tsc --project tsconfig.cjs.json",
		"build:esm": "tsc --project tsconfig.esm.json",
		"copy-assets": "copyfiles -u 1 src/assets/**/* src/styles/**/* dist",
		"obfuscate": "javascript-obfuscator dist --output dist --config obfuscator-config.json",
		"dev": "nodemon --watch src --ext ts,tsx --exec \"npm run build\"",
		"prepublishOnly": "copyfiles -f package.publish.json dist/package.json"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"keywords": [
		"dothedue",
		"Aditya Singh"
	],
	"author": "Aditya Singh",
	"license": "ISC",
	"description": "",
	"dependencies": {
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"autoprefixer": "^10.4.20",
		"motion": "^11.15.0",
		"postcss": "^8.4.49",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-icons": "^5.4.0",
		"tailwind-scrollbar": "^3.1.0",
		"tailwindcss": "^3.4.15",
		"typescript": "^5.7.2"
	},
	"devDependencies": {
		"@types/node": "^22.10.1",
		"copyfiles": "^2.4.1",
		"javascript-obfuscator": "^4.1.1",
		"nodemon": "^3.1.9"
	}
}
